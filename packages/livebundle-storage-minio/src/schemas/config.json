{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://electrode.io/schemas/storage-minio.json",
  "type": "object",
  "properties": {
    "endPoint": {
      "type": "string"
    },
    "port": {
      "type": "number"
    },
    "useSSL": {
      "type": "boolean"
    },
    "accessKey": {
      "type": "string"
    },
    "secretKey": {
      "type": "string"
    },
    "bucketName": {
      "type": "string"
    }
  },
  "required": ["endPoint", "port", "accessKey", "secretKey", "bucketName"]
}
